<div class="container">
<div class="contents row">
<h3 >内容編集</h3>
  <div class="command">
    <%= form_with(model: @tweet, local: true) do |form| %>
      <%= form.text_area :text, placeholder: "書いたら更新ボタンを押してね", rows: "7", cols: "40" %>
      <%= form.submit "更新" %>
    <% end %>
  </div>
</div>
</div>